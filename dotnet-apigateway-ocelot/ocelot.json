{
  "Routes": [
    {
      "ServiceName": "SEN300ECOMCatalogServiceAPI",
      "DownstreamPathTemplate": "/{everything}",
      "UpstreamPathTemplate": "/catalogserviceapi/{everything}",
      "UpstreamHttpMethod": [
        "GET",
        "POST",
        "PUT",
        "DELETE"
      ],
      "FileCacheOptions": {
        "TtlSeconds": 15
      },
      "LoadBalancerOptions": {
        "Type": "LeastConnection",
        "Key": null,
        "Expiry": 0
      }
    },
    {
      "ServiceName": "SEN300ECOMBasketServiceAPI",
      "DownstreamPathTemplate": "/{everything}",
      "UpstreamPathTemplate": "/basketserviceapi/{everything}",
      "UpstreamHttpMethod": [
        "GET",
        "POST",
        "PATCH",
        "DELETE"
      ],
      "FileCacheOptions": {
        "TtlSeconds": 15
      },
      "LoadBalancerOptions": {
        "Type": "LeastConnection",
        "Key": null,
        "Expiry": 0
      }
    },
    {
      "ServiceName": "SEN300ECOMAccountServiceAPI",
      "DownstreamPathTemplate": "/{everything}",
      "UpstreamPathTemplate": "/accountserviceapi/{everything}",
      "UpstreamHttpMethod": [
        "GET",
        "POST",
        "PUT",
        "DELETE"
      ],
      "FileCacheOptions": {
        "TtlSeconds": 15
      },
      "LoadBalancerOptions": {
        "Type": "LeastConnection",
        "Key": null,
        "Expiry": 0
      }
    },
    {
      "ServiceName": "SEN300ECOMCheckOutServiceAPI",
      "DownstreamPathTemplate": "/{everything}",
      "UpstreamPathTemplate": "/checkoutserviceapi/{everything}",
      "UpstreamHttpMethod": [
        "GET",
        "POST",
        "PUT",
        "DELETE"
      ],
      "FileCacheOptions": {
        "TtlSeconds": 15
      },
      "LoadBalancerOptions": {
        "Type": "LeastConnection",
        "Key": null,
        "Expiry": 0
      }
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://SEN300APIGatewayOcelot:8080",
    "RequestIdKey": "OcRequestId",
    "DownstreamScheme": "http",
    "ReRoutesCaseSensitive": false,
    "UseServiceDiscovery": true,
    "ServiceDiscoveryProvider": {
      "Host": "SEN300ECOMEurekaRegistry",
      "Port": 8761,
      "Type": "Eureka"
    }
  }
}